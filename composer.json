{
    "name": "homedesignshops/laravel-zendesk",
    "description": "A Laravel Zendesk wrapper for the Zendesk API client",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "zendesk/zendesk_api_client_php": "^2.2"
    },
    "require-dev": {
        "php": "^7.1.3",
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "HomeDesignShops\\Zendesk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HomeDesignShops\\Zendesk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HomeDesignShops\\Zendesk\\ZendeskServiceProvider"
            ]
        }
    },
}
